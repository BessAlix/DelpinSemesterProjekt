@model IEnumerable<DelpinBooking.Models.Machine>

@{
    ViewData["Title"] = "Maskiner";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Warehouse)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Warehouse.City)
                    @Html.DisplayFor(modelItem => item.Warehouse.PostCode)

                </td>
            </tr>
        }
    </tbody>
</table>